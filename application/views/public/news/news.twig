{% extends 'layouts/app.twig' %}

{% block title %}Новости{% endblock %}

	{% block section %}
		<div class="container">
			<div class="row">
			{% for item in articles %}
				<h3><a href="{{ path_for('article.getOne', {id: item.id } ) }}">{{ item.title }}</a></h3><br><p>{{ item.discription }}</p>
			{% endfor %}
			</div>
		</div>
		
		
	{% endblock %}